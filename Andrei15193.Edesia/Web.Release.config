<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<EdesiaSettings xdt:Transform="Replace">
		<RegistrationSettings RegistrationKeyLength="7" RegistrationKeyHoursTimeout="24" />
		<StorageSettings UseCloudSettings = "true" />
		<EmailSettings UseCloudSettings = "true" />
	</EdesiaSettings>
	<system.web>
		<customErrors mode="On" defaultRedirect="/" xdt:Transform="Replace" />
	</system.web>
</configuration>