@using Andrei15193.Edesia.Resources;
@{
	ViewBag.Title = DeliveryControllerStrings.DefaultViewTitle;
}
<h1>@DeliveryControllerStrings.DefaultViewTitle</h1>
<div class="grid fluid">
	<div class="row">
		<div class="span4">
			@{
				Html.RenderAction("Default", "DeliveryZone");
			}
		</div>
		<div class="span4">
			@{
				Html.RenderAction("Default", "DeliveryTask");
			}
		</div>
		<div class="span4">
			@{
				Html.RenderAction("Default", "Order");
			}
		</div>
	</div>
</div>